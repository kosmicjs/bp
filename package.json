{
  "name": "@kosmic/core",
  "version": "0.0.0",
  "description": "Strongly typed Koa web and api framework with all the basics built in and minimal opinions",
  "bugs": {
    "url": "https://github.com/spence-s/kosmic/issues"
  },
  "repository": {
    "url": "https://github/com/spence-s/kosmic"
  },
  "license": "MIT",
  "author": {
    "name": "Spencer Snyder",
    "email": "sasnyde2@gmail.com",
    "url": "https://spencersnyder.io"
  },
  "type": "module",
  "imports": {
    "#kosmic/render": "./packages/render/middleware.js",
    "#kosmic/logger": "./packages/logger/console-logger.js",
    "#kosmic/*": "./packages/*/index.js"
  },
  "main": "dist/src/index.js",
  "source": "./src/app/client/main.ts",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rimraf -rf ./dist",
    "compile": "tsc -p tsconfig.json",
    "dev": "node --loader ts-node/esm/transpile-only ./packages/dev/index.ts & vite",
    "prepare": "husky install",
    "prepublishOnly": "npm run build",
    "test": "ava"
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-packagejson",
      "@prettier/plugin-pug"
    ]
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "argon2": "^0.31.1",
    "bootstrap": "^5.3.2",
    "chokidar": "^3.5.3",
    "eslint-config-xo-react": "^0.27.0",
    "execa": "^8.0.1",
    "fast-glob": "^3.3.1",
    "htmx.org": "^1.9.6",
    "http-terminator": "^3.2.0",
    "kcors": "^2.2.2",
    "koa": "npm:@kosmic/koa@latest",
    "koa-404-handler": "^0.1.0",
    "koa-bodyparser": "^4.4.1",
    "koa-compose": "npm:@kosmic/compose@latest",
    "koa-conditional-get": "^3.0.0",
    "koa-etag": "^4.0.0",
    "koa-json": "^2.0.2",
    "koa-passport": "^6.0.0",
    "koa-pino-logger": "^4.0.0",
    "koa-requestid": "^2.1.0",
    "koa-response-time": "^2.1.0",
    "koa-static": "^5.0.0",
    "kysely": "^0.26.3",
    "lodash": "^4.17.21",
    "passport-google-oauth20": "^2.0.0",
    "passport-local": "^1.0.0",
    "path-to-regexp": "^6.2.1",
    "pg": "^8.11.3",
    "pino": "^8.16.0",
    "pino-http": "^8.5.0",
    "pino-princess": "^0.5.0",
    "preact": "^10.18.1",
    "preact-render-to-string": "^6.2.2",
    "pug": "^3.0.2",
    "short-unique-id": "^5.0.3",
    "short-uuid": "^4.2.2",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "sql-template-strings": "^2.2.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.2",
    "@commitlint/config-conventional": "^17.7.0",
    "@prettier/plugin-pug": "^3.0.0",
    "@types/bootstrap": "^5.2.7",
    "@types/kcors": "^2",
    "@types/koa-bodyparser": "^4",
    "@types/koa-conditional-get": "^2.0.1",
    "@types/koa-etag": "^3.0.1",
    "@types/koa-passport": "^6.0.1",
    "@types/koa-pino-logger": "^3.0.2",
    "@types/koa-response-time": "^2",
    "@types/koa-static": "^4.0.2",
    "@types/koa__router": "^12.0.1",
    "@types/node": "~20.8.4",
    "@types/passport-google-oauth20": "^2.0.12",
    "@types/passport-local": "^1.0.36",
    "@types/pg": "^8.10.5",
    "@types/pug": "^2.0.7",
    "ava": "^5.3.1",
    "eslint": "^8.51.0",
    "eslint-config-preact": "^1.3.0",
    "eslint-plugin-react": "^7.33.2",
    "got": "^13",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "npm-package-json-lint": "^7.0.0",
    "npm-package-json-lint-config-default": "^6.0.0",
    "parcel": "^2.9.3",
    "prettier": "^3.0.3",
    "prettier-plugin-packagejson": "^2.4.6",
    "rimraf": "^5.0.5",
    "sass": "^1.69.2",
    "ts-node": "^10.9.1",
    "type-fest": "^4.4.0",
    "typescript": "~5.2.2",
    "vite": "^4.4.11",
    "xo": "^0.56.0"
  }
}
