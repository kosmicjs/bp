{
  "name": "@kosmic/core",
  "version": "0.0.0",
  "description": "Strongly typed Koa web and api framework with all the basics built in and minimal opinions",
  "bugs": {
    "url": "https://github.com/spence-s/kosmic/issues"
  },
  "repository": {
    "url": "https://github/com/spence-s/kosmic"
  },
  "license": "MIT",
  "author": {
    "name": "Spencer Snyder",
    "email": "sasnyde2@gmail.com",
    "url": "https://spencersnyder.io"
  },
  "type": "module",
  "imports": {
    "#config": "./src/config/index.js"
  },
  "main": "dist/src/index.js",
  "source": "./src/app/client/main.ts",
  "types": "dist/src/index.d.ts",
  "bin": {
    "kosmic": "dist/packages/dev/cli.js"
  },
  "scripts": {
    "analyze": "vite-bundle-visualizer",
    "build": "npm run clean && npm run compile",
    "check": "tsc -p tsconfig.json --noEmit",
    "check:watch": "tsc -p tsconfig.json --noEmit --watch",
    "clean": "rimraf -rf ./dist & rimraf -rf ./src/public/assets & rimraf -rf ./src/public/.vite",
    "compile": "tsc -p tsconfig.json && vite build && cp -rp src/public ./dist/src",
    "dev": "kosmic dev",
    "migrate": "vite-node ./src/db/migrate up",
    "migrate:down": "vite-node ./src/db/migrate down",
    "prepare": "husky || true",
    "prepublishOnly": "npm run build",
    "start": "NODE_ENV=production node dist/src/index.js",
    "test": "vitest test/server.test."
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-packagejson"
    ]
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "@stripe/stripe-js": "^4.0.0",
    "argon2": "^0.40.3",
    "bootstrap": "^5.3.3",
    "camelcase": "^8.0.0",
    "chokidar": "^3.6.0",
    "clsx": "^2.1.1",
    "copy-to-clipboard": "^3.3.3",
    "dedent": "^1.5.3",
    "defaults": "^3.0.0",
    "dotenv": "^16.4.5",
    "escape-goat": "^4.0.0",
    "execa": "^9.3.0",
    "fast-glob": "^3.3.2",
    "globby": "^14.0.1",
    "got": "^14.4.1",
    "highlight.js": "^11.9.0",
    "htmx.org": "^1.9.12",
    "humanize-string": "^3.0.0",
    "kcors": "^2.2.2",
    "koa": "^2.15.3",
    "koa-404-handler": "^0.1.0",
    "koa-bodyparser": "^4.4.1",
    "koa-compose": "^4.1.0",
    "koa-conditional-get": "^3.0.0",
    "koa-etag": "^4.0.0",
    "koa-json": "^2.0.2",
    "koa-passport": "^6",
    "koa-pino-logger": "^4.0.0",
    "koa-requestid": "^2.2.1",
    "koa-response-time": "^2.1.0",
    "koa-session": "^6.4.0",
    "koa-static": "^5.0.0",
    "kysely": "^0.27.3",
    "lodash": "^4.17.21",
    "passport-google-oauth20": "^2.0.0",
    "passport-local": "^1.0.0",
    "path-to-regexp": "^7.0.0",
    "pg": "^8.12.0",
    "pino": "9.2.0",
    "pino-http": "^10.1.0",
    "pino-princess": "file:../../pino-princess",
    "preact": "^10.22.0",
    "preact-render-to-string": "^6.5.5",
    "short-uuid": "^5.2.0",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5",
    "sql-template-strings": "^2.2.2",
    "stripe": "^16.1.0",
    "titleize": "^4.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@kosmic/types": "^0.0.3",
    "@prettier/plugin-pug": "^3.0.0",
    "@types/bootstrap": "^5.2.10",
    "@types/kcors": "^2",
    "@types/koa-bodyparser": "^4",
    "@types/koa-conditional-get": "^2.0.3",
    "@types/koa-etag": "^3.0.3",
    "@types/koa-passport": "^6.0.3",
    "@types/koa-pino-logger": "^3.0.4",
    "@types/koa-response-time": "^2",
    "@types/koa-session": "^6.4.5",
    "@types/koa-static": "^4.0.4",
    "@types/node": "~20.14.9",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/passport-local": "^1.0.38",
    "@types/pg": "^8.11.6",
    "eslint-config-preact": "^1.4.0",
    "eslint-config-xo-react": "^0.27.0",
    "eslint-plugin-react": "^7.34.3",
    "husky": "^9.0.11",
    "kosmic": "file:.",
    "lint-staged": "^15.2.7",
    "meow": "^13.2.0",
    "npm-package-json-lint": "^8.0.0",
    "npm-package-json-lint-config-default": "^7.0.1",
    "prettier": "^3.3.2",
    "prettier-plugin-packagejson": "^2.5.0",
    "rimraf": "^5.0.7",
    "sass": "^1.77.6",
    "type-fest": "^4.20.1",
    "typed-htmx": "^0.3.1",
    "typescript": "~5.5.2",
    "vite": "^5.3.2",
    "vite-bundle-visualizer": "^1.2.1",
    "vite-node": "^1.6.0",
    "vitest": "^1.6.0",
    "xo": "^0.58.0"
  }
}
